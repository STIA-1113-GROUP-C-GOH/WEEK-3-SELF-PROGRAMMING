package selfProgramming;

public class Precision {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println((35.0 + 22.4 + 11.9) / 3.0);
		System.out.println((35 + 22.4 + 11) / 3);
		System.out.println((35 + 22 + 11) / 3);
		System.out.println((35 + 22 + 11) / 3.0);//The decimal will affect the output




	}

}

Output
23.099999999999998
22.8
22
22.666666666666668
